{
  "name": "clapcoin-subgraph",
  "version": "1.0.0",
  "description": "The Graph subgraph for ClapCoin social tipping platform",
  "repository": "https://github.com/your-org/clapcoin",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy",
    "create-local": "graph create --node http://localhost:8020/ clapcoin/tips",
    "remove-local": "graph remove --node http://localhost:8020/ clapcoin/tips",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 clapcoin/tips",
    "deploy-hosted": "graph deploy --product hosted-service your-github-username/clapcoin-tips",
    "deploy-studio": "graph deploy --studio clapcoin-tips"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.56.0",
    "@graphprotocol/graph-ts": "0.30.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}